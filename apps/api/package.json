    "class-validator": "^0.14.3",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.3.1",
    "handlebars": "^4.7.8",
    "minio": "^8.0.5",
    "mysql2": "^3.17.0",
